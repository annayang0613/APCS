#include <bits/stdc++.h>
using namespace std;
bool check(const vector<vector<int>> &a,int x,int y, int n,int m){
  int total=0;
  for(int i=0; i<n; i++) {
    for(int j=0; j<m; j++) {
      if(abs(i-x)+abs(j-y) <= a[x][y]) {
        total +=a[i][j];
      }
    }
  }
  if(total%10 == a[x][y]) return 1;
  else return 0;
}

int main() {
  int n, m;
  cin >> n >> m;
  vector<vector<int>> map(n, vector<int>(m, 0));
  vector<pair<int, int>> ans;
  for(int i=0; i<n; i++) {
    for(int j=0; j<m; j++) {
      cin >> map[i][j];
    }
  }
  for(int i=0; i<n; i++) {
    for(int j=0; j<m; j++) {
      if(check(map, i, j, n, m)) ans.emplace_back(i, j);
    }
  }
  sort(ans.begin(), ans.end());
  cout << ans.size() << "\n";
  for(int i=0; i<ans.size(); i++) {
    cout << ans[i].first << " " << ans[i].second << "\n";
  }
  return 0;
}
