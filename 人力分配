#include <bits/stdc++.h>
using namespace std;

int main() {
  int A1, B1, C1, A2, B2, C2, n, large;
  cin >> A1 >> B1 >> C1 >> A2 >> B2 >> C2 >> n;
  int X1=0, X2=n;
  for(int i=0; i<n+1; i++) {
    long long Y1 = A1*X1*X1+B1*X1+C1;
    long long Y2 = A2*X2*X2+B2*X2+C2;
    if(i==0) { large = Y1+Y2; X1++; X2--; continue; }
    if(Y1+Y2 > large) large=Y1+Y2;
    X1++;
    X2--;
  }
  cout << large;
  return 0;
}
