#include <iostream>
#include <cmath>
using namespace std;

int main() {
	int a, b, c, x1=00, x2=00;
	cin >> a >> b >>c;
	x1=(-b+sqrt(b*b-4*a*c))/(a*2);
	x2=(-b-sqrt(b*b-4*a*c))/(a*2);
	if(b*b-4*a*c>0) cout << "Two different roots x1=" << x1 << " , x2=" << x2;
	else if(b*b-4*a*c==0) cout << "Two same roots x=" << x1;
	else cout << "No real root";
	return 0;
}
